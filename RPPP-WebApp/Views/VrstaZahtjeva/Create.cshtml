@model RPPP_WebApp.Model.VrstaZahtjeva

@{
    ViewBag.Title = "Nova vrsta zahtjeva";
}

<h2>Unos novog koncesionara</h2>

<form asp-action="Create" method="post">
    <div asp-validation-summary="All"></div>
    <div class="form-group">
        <label asp-for="NazivVrsteZahtjeva"></label>
        <div><span asp-validation-for="NazivVrsteZahtjeva" class="text-danger"></span></div>
        <input asp-for="NazivVrsteZahtjeva" class="form-control" />
    </div>
   
    <div class="text-center">
        <button class="btn btn-primary" type="submit">Dodaj</button>
        <a asp-action="Index" class="btn btn-default">Odustani</a>
    </div>
</form>

@section scripts {
    <partial name="IncludeValidation" />
}
